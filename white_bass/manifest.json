{
  "manifest_version": 2,

  "name": "white_bass",
  "description": "This extension records form e",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "storage",
    "unlimitedStorage"
  ],

  "browser_action": {
    "default_icon": "images/favicon.png",
    "default_popup": "barracuda.html"
  },

  "icons": { 
    "16": "images/favicon.png",
    "48": "images/favicon_48.png",
    "128": "images/favicon_128.png" 
  },

  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js": ["jquery-2.1.4.min.js","salmon.js"]
    }
  ]

}
